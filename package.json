{
  "name": "historic-fuel-stations-fetcher",
  "version": "1.0.0",
  "description": "Fetches historic values from the Spanish government fuel stations API",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "prepare": "ts-patch install",
    "bundle": "esbuild ./dist/index.js --bundle --platform=node --outfile=output/historic_fuel_stations_fetcher.js"
  },
  "author": "JGeek00",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.7",
    "esbuild": "^0.24.0",
    "luxon": "^3.5.0",
    "ts-node": "^10.9.2",
    "typia": "^6.10.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.6.1",
    "ts-patch": "^3.2.1",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2"
  }
}
